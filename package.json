{
    "name": "react-ts-quick-starter",
    "version": "1.0.0",
    "description": "Quick create react + typescript project development enviroment and scaffold for developing npm package components",
    "main": "index.js",
    "scripts": {
        "start": "cross-env NODE_ENV=development webpack serve --progress --color --config ./scripts/config/webpack.dev.js",
        "build": "npm run build:pro",
        "build:dev": "cross-env NODE_ENV=development webpack --config ./scripts/config/webpack.dev.js",
        "build:pro": "cross-env NODE_ENV=production webpack --config ./scripts/config/webpack.prod.js",
        "lint": "npm run lint-eslint && npm run lint-stylelint",
        "lint-eslint": "eslint -c .eslintrc.js --ext .ts,.tsx,.js src",
        "lint-stylelint": "stylelint --config .stylelintrc.js src/**/*.{less,css,scss}",
        "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
        "prepare": "husky install"
    },
    "lint-staged": {
        "*.{ts,tsx,js}": [
            "eslint --config .eslintrc.js"
        ],
        "*.{css,less,scss}": [
            "stylelint --config .stylelintrc.js"
        ],
        "*.{ts,tsx,js,json,html,yml,css,less,scss,md}": [
            "prettier --write"
        ]
    },
    "repository": {
        "url": "https://github.com/Pzx1997/react-ts-quick-starter"
    },
    "keywords": [
        "react-project",
        "typescript-project",
        "react-typescript",
        "react-ts-quick-starter"
    ],
    "author": "pxyo",
    "license": "MIT",
    "bugs": {},
    "homepage": "",
    "browserslist": [
        ">0.2%",
        "not dead",
        "ie >= 9",
        "not op_mini all"
    ],
    "devDependencies": {
        "@babel/core": "^7.14.3",
        "@babel/plugin-transform-runtime": "^7.14.3",
        "@babel/preset-env": "^7.14.4",
        "@babel/preset-react": "^7.13.13",
        "@babel/preset-typescript": "^7.13.0",
        "@commitlint/cli": "^12.1.4",
        "@commitlint/config-conventional": "^12.1.4",
        "@types/react": "^17.0.9",
        "@types/react-dom": "^17.0.6",
        "@types/webpack-env": "^1.16.0",
        "@typescript-eslint/eslint-plugin": "^4.26.0",
        "@typescript-eslint/parser": "^4.26.0",
        "autoprefixer": "^10.2.6",
        "babel-loader": "^8.2.2",
        "clean-webpack-plugin": "^4.0.0-alpha.0",
        "conventional-changelog-cli": "^2.1.1",
        "copy-webpack-plugin": "^9.0.0",
        "cross-env": "^7.0.3",
        "css-loader": "^5.2.6",
        "eslint": "^7.27.0",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-import-resolver-typescript": "^2.4.0",
        "eslint-plugin-import": "^2.23.4",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-prettier": "^3.4.0",
        "eslint-plugin-promise": "^5.1.0",
        "eslint-plugin-react": "^7.24.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "eslint-plugin-unicorn": "^32.0.1",
        "file-loader": "^6.2.0",
        "fork-ts-checker-webpack-plugin": "^6.2.10",
        "glob": "^7.1.7",
        "hard-source-webpack-plugin": "^0.13.1",
        "html-webpack-plugin": "^5.3.1",
        "husky": "^6.0.0",
        "less": "^4.1.1",
        "less-loader": "^9.0.0",
        "lint-staged": "^11.0.0",
        "mini-css-extract-plugin": "^1.6.0",
        "optimize-css-assets-webpack-plugin": "^6.0.0",
        "postcss-flexbugs-fixes": "^5.0.2",
        "postcss-loader": "^5.3.0",
        "postcss-normalize": "^10.0.0",
        "postcss-preset-env": "^6.7.0",
        "prettier": "^2.3.0",
        "purgecss-webpack-plugin": "^4.0.3",
        "style-loader": "^2.0.0",
        "stylelint": "^13.13.1",
        "stylelint-config-prettier": "^8.0.2",
        "stylelint-config-rational-order": "^0.1.2",
        "stylelint-config-standard": "^22.0.0",
        "stylelint-declaration-block-no-ignored-properties": "^2.3.0",
        "stylelint-order": "^4.1.0",
        "terser-webpack-plugin": "^5.1.3",
        "typescript": "^4.3.2",
        "url-loader": "^4.1.1",
        "webpack": "^5.38.1",
        "webpack-bundle-analyzer": "^4.4.2",
        "webpack-cli": "^4.7.0",
        "webpack-dev-server": "^3.11.2",
        "webpack-merge": "^5.7.3",
        "webpackbar": "^5.0.0-3"
    },
    "dependencies": {
        "@babel/runtime": "^7.14.0",
        "@babel/runtime-corejs3": "^7.14.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2"
    }
}
